<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.help.LoginController" prefHeight="400.0" prefWidth="600.0">
    <Text fx:id="title" text = "Login"/>
    <Label fx:id="usernameText" text = "Username:"/>
    <TextField fx:id="username" maxWidth="300"/>
    <Label fx:id="PasswordText" text = "Password"/>
    <TextField fx:id="password" maxWidth="300"/>
    <Button fx:id="homePage" text = "Login" onAction="#switchToHomePage"/>
</VBox>
