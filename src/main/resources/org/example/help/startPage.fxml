<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.help.StartPageController">
   <children>
      <Text fx:id="title" text = "Welcome to Quiz Generator" />
      <Button fx:id="loginButton" text="Login" onAction="#switchToLogin"/>
      <Button fx:id="signUpButton" text="Sign Up" onAction="#switchToSignUp"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
