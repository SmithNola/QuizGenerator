<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.quizgen.controller.ScoreController" prefHeight="400.0" prefWidth="600.0" stylesheets="@../stylesheet/scoresheet.css">
    <VBox>
        <Label fx:id="quizName"/>
        <Label fx:id="score"/>
    </VBox>
    <VBox fx:id="results">

    </VBox>
    <HBox>
        <Button text="Done" onAction="#switchToPlayView"/>
        <Button text="Sign Out" onAction="#returnToStart"/>
    </HBox>
</VBox>
