<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<ScrollPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.quizgen.controller.DisplayQuizzesController" prefHeight="400.0" prefWidth="600.0" stylesheets="@../stylesheet/displayquizzesstylesheet.css">
<VBox fx:id="overall">
    <HBox>
        <Label text="Name"/>
        <Label text="Genre"/>
        <Label text="Creator"/>
        <Label text="Date Created"/>
    </HBox>
    <VBox fx:id="showQuizzes">

    </VBox>
    <HBox fx:id="buttons" spacing="10">
        <children>
            <Button fx:id="create" text="Create" onAction="#switchToQuizSettings"/>
            <Button text="Home" onAction="#returnToHome"/>
            <Button text="Sign Out" onAction="#returnToStart"/>
        </children>
    </HBox>
</VBox>
</ScrollPane>
