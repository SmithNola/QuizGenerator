<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.quizgen.controller.StartPageController" stylesheets="@../stylesheet/stylesheet.css">
   <Text fx:id="title" text="Welcome to Quiz Generator"/>
   <Button fx:id="loginButton" text="Login" onAction="#switchToLogin"/>
   <Button fx:id="signUpButton" text="Sign Up" onAction="#switchToSignUp"/>
   <Text fx:id="errorMessage"/>
</VBox>
