<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<ScrollPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.quizgen.controller.PlayingController" prefHeight="400.0" prefWidth="600.0" stylesheets="@../stylesheet/playingsheet.css">
    <VBox>
        <Label fx:id="quizName"/>
        <VBox fx:id="displayQuiz">

        </VBox>
        <HBox fx:id="buttons">
            <Button fx:id="saveButton" text="Save" onAction="#finishPlaying"/>
            <Button fx:id="cancelButton" text="Cancel" onAction="#cancelPlaying"/>
        </HBox>
    </VBox>
</ScrollPane>
